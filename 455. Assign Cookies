class Solution:
    def findContentChildren(self, g: List[int], s: List[int]) -> int:
        g.sort()
        s.sort()
        
        child = 0  # index for children
        cookie = 0 # index for cookies
        
        while child < len(g) and cookie < len(s):
            if s[cookie] >= g[child]:
                # assign cookie to child
                child += 1
            # move to next cookie anyway
            cookie += 1
        
        return child
